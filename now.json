{
  "name": "citizenscience-v2",
  "version": 2,
  "routes": [
    {
      "src": "/(js|css|img)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "alias": "v1.staging.citizenscience.ch"
}


